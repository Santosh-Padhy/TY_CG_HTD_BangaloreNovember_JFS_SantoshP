package dsaproj;

public class AddingElement {

	public static void main(String[] args) {
		
	}

	public static long addUpto(long number)
	{
		long total = 0l;
		for(long i = 0;i<=number;i++) {
			total= total+i;
		
		
	}
		return total;
		
}
	public static long addUptoquick(Long number) {
		long total = 0l;
		return number*(number+1)/2;
	}
	
	
}
